@{
    ViewBag.Title = "Confirmação de Email";
}


<section id="Confirmacao" class="section section-gray">


    <div class="container">

        <h2 class="section-heading text-center">Conta Coruja</h2>


        <div class="row">
            <div class="col-sm-6 col-md-6">
                <div class="alert-message alert-message-warning">
                    <img src="~/Content/CorujaImages/avatar2.png" alt="Coruja">
                    <h4>
                        Obrigado por confirmar seu Email. @Html.ActionLink("Entrar no espaço Coruja", "Login", "Conta", routeValues: null, htmlAttributes: new { @class = "a"})
                    </h4>

                </div>
            </div>
        </div>

    </div>

</section>

